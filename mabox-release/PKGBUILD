pkgname=mabox-release
pkgver=16.11
pkgrel=1
pkgdesc="Mabox's release definition"
arch=("any")
url="http://maboxlinux.org/"
license=('GPL2')
depends=('lsb-release')
source=('lsb-release')
conflicts=('manjaro-release' 'manjaro-release-dev')
install="mabox-release.install"
sha256sums=(SKIP)

package() {
    # Copy files
    mkdir -p ${pkgdir}/etc
    install -m644 ${srcdir}/lsb-release ${pkgdir}/etc/

}
